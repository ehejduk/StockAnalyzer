{
  "periods": {
    "_xpath": "//th[contains(@class,'thq')]/text()[1]",
    "_transformations": [
      "TrimTransformation"
    ]
  },
  "rows": {
    "_xpath": "//tr[contains(@data-field,'Cashflow')]",
    "label": ".//@data-field",
    "description": ".//td[@class='f']//a",
    "vals": {
      "_xpath": ".//td[contains(@class, 'h')]//span[@class='value']//span//span",
      "_transformations": [
        "StripSpacesTransform"
      ]
    }
  }

}


